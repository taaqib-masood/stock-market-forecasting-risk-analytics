# 02_feature_engineering.ipynb
import pandas as pd

# Load data
data = pd.read_csv('../data/raw/AAPL_sample.csv', parse_dates=['Date'])
data.set_index('Date', inplace=True)

# Create moving averages
data['MA_5'] = data['Close'].rolling(window=5).mean()
data['MA_10'] = data['Close'].rolling(window=10).mean()

# Create daily returns
data['Daily_Return'] = data['Close'].pct_change()

# Drop NaN values from rolling windows
data.dropna(inplace=True)

# Save processed data
data.to_csv('../data/processed/AAPL_processed.csv')

print("Processed data saved to '../data/processed/AAPL_processed.csv'")
print(data.head())
